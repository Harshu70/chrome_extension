{
  "manifest_version": 3,
  "name": "OpenAI Assistant",
  "version": "1.0",
  "description": "A Chrome extension to interact with OpenAI using your API key.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": ["src/Content.tsx"],
      "matches": ["*://leetcode.com/*", "*://www.leetcode.com/*"]
    }
  ],
  "background": {
    "service_worker": "src/background.js"
  }
}
